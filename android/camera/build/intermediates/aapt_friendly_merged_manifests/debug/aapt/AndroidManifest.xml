<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mobilemd.cameralibrary"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="28" />

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true" >
        <activity android:name="com.mobilemd.cameralibrary.CameraActivity" />
        <activity
            android:name="com.mobilemd.cameralibrary.VideoActivity"
            android:configChanges="orientation|screenSize|keyboardHidden" />
        <activity
            android:name="com.mobilemd.cameralibrary.SoundActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>